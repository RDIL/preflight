<p id="notice"><%= notice %></p>

<h3>
  <%= link_to checklist.github_repository.github_full_name, github_repository_path(checklist.github_repository) %>
</h3>

<h3>
  <%= checklist.name %>
</h3>

<div class="row">
  <div class="col-xs-10">
    <%= link_to 'Edit', edit_checklist_path(checklist) %>
    <%= link_to 'Back', checklists_path %>
  </div>
</div>

<%= render 'checklist_items', checklist: checklist %>
